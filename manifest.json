{
    "name":"Historia Quick Launcher",
    "version":"1.1.0",
    "description":"Quick Launch App for Historians",
    "homepage_url": "https://extension-historia.github.io/",
    "manifest_version":3,
    "icons": {
      "16": "icons/icon_16px.png",
      "32": "icons/icon_32px.png",
      "48": "icons/icon_48px.png",
      "128": "icons/icon_128px.png"
    },
    "action":{
        "default_icon":"icons/icon_128px.png",
        "default_popup":"popup.html"
    },
    "permissions":["activeTab"]
}
